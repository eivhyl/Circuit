{
  "name": "circuit",
  "version": "0.9.0",
  "description": "Circuit is an IRC client built with React and Electron.",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "production": "webpack --config webpack.production.config.js",
    "dev": "cross-env NODE_ENV=development node app/dev-server.js",
    "dev-server": "webpack-dev-server --config webpack.development.config.js --port 1337 --progress --colors --hot --inline --watch",
    "lint": "tslint app/src/** --type-check -p ./"
  },
  "repository": "https://github.com/eivhyl/Circuit",
  "keywords": [
    "Electron",
    "Typescript",
    "React",
    "Redux",
    "IRC"
  ],
  "author": "GitHub",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^5.1.4",
    "electron-prebuilt-compile": "^2.0.0",
    "express": "^4.14.0",
    "irc": "^0.5.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "@types/irc": "^0.3.32",
    "@types/node": "^7.0.31",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-redux": "*",
    "@types/redux-actions": "^2.2.4",
    "@types/webpack": "^4.1.4",
    "@types/webpack-env": "^1.12.1",
    "@types/ws": "^5.1.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "normalize.css": "^8.0.0",
    "react-hot-loader": "^4.1.2",
    "react-redux-typescript": "^2.3.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-loader": "*",
    "tslint": "latest",
    "typescript": "latest",
    "webpack": "^4.6.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^4.1.0"
  }
